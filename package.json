{
  "name": "nuxt-property-decorator",
  "version": "2.1.1",
  "description": "Property decorators for Nuxt",
  "main": "lib/nuxt-property-decorator.umd.js",
  "keywords": [
    "vue",
    "typescript",
    "nuxt",
    "decorator"
  ],
  "author": "davidbernal",
  "contributors": [
    {
      "name": "Tony BRIET",
      "email": "tony.briet@viacesi.fr"
    },
    {
      "name": "HG",
      "email": "husayt@gmail.com"
    }
  ],
  "license": "MIT",
  "files": [
    "lib"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "gitHooks": {
    "pre-commit": "npm run build"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.12",
    "vue-class-component": "7.0.1",
    "vue-property-decorator": "^7.3.0",
    "vuex": "^3.1.0",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@std/esm": "^0.26.0",
    "@types/jest": "^24.0.9",
    "@types/node": "^10.12.27",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "rollup": "^1.3.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "standard-version": "^5.0.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333",
    "vue": "^2.6.7",
    "vue-jest": "^3.0.3",
    "vue-template-compiler": "^2.6.7",
    "yorkie": "^2.0.0"
  },
  "typings": "./lib/nuxt-property-decorator.d.ts",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/nuxt-property-decorator.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-community/nuxt-property-decorator/issues"
  },
  "homepage": "https://github.com/nuxt-community/nuxt-property-decorator#readme"
}
